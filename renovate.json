{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"local>naa0yama/renovate-config"
	],
	"customManagers": [
		{
			"customType": "regex",
			"managerFilePatterns": [
				"/^\\.github/workflows/.+\\.ya?ml$/"
			],
			"matchStrings": [
				"# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)(?:\\s+registryUrl=(?<registryUrl>.*?))?\\n\\s+\\w+:\\s*\"?(?<currentValue>[\\d.]+)\"?"
			]
		}
	]
}
